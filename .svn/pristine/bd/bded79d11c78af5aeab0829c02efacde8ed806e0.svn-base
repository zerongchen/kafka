<?xml version="1.0" encoding="UTF-8"?>
<config>
	<!-- #kafka config -->
	<kafka.zks>hadoop-r720-3:2181</kafka.zks>
    <kafka.brokerZkPath>/kafka_2/brokers</kafka.brokerZkPath>
    <!-- #hdfs config -->
    <hdfs.uri>hdfs://hadoop-r720-4:8020/</hdfs.uri>
    <hdfs.user>hadoop</hdfs.user>
    <!-- #storm config -->
    <storm.zks>hadoop-r720-1,hadoop-r720-3,hadoop-r720-4</storm.zks>
    <storm.zkport>2181</storm.zkport>
    <storm.zkRoot>/storm</storm.zkRoot>
    <storm.workerNum>3</storm.workerNum>
    <!-- #hive config -->
    <hive.jdbc.url>jdbc:hive2://hadoop-r720-4:10000/cmcc</hive.jdbc.url>
    <hive.jdbc.driver>org.apache.hive.jdbc.HiveDriver</hive.jdbc.driver>
    <hive.jdbc.user>hadoop</hive.jdbc.user>
    <hive.jdbc.passwd></hive.jdbc.passwd>
	
	<!-- # catalog config -->
	<fiveMinDatalist>
		<item>
			<topic>cmccServerbuilder</topic>
			<spout.parallelism_hint>1</spout.parallelism_hint>
			<bolt.parallelism_hint>2</bolt.parallelism_hint>
			<tablename>hive_serverbuild_flow_min</tablename>
			<hdfs.path>/user/hive/warehouse/cmcc.db/hive_serverbuild_flow_min/</hdfs.path>
			<tick.tuple.interval>300</tick.tuple.interval>
			<sync.count>300000</sync.count>
		</item>
		  
		<item>
			<topic>cmccProtocol</topic>
			<spout.parallelism_hint>1</spout.parallelism_hint>
			<bolt.parallelism_hint>2</bolt.parallelism_hint>
